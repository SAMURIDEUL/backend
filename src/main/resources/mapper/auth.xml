<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.samuL.auth.mapper.AuthMapper">

    <select id="findEmail" parameterType="com.example.samuL.auth.dto.LoginUserDto">
        select *
        from users
        where email = #{email}
    </select>


</mapper>